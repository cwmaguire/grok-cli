{
  "name": "@vibe-kit/grok-cli",
  "version": "0.0.35",
  "description": "An open-source AI agent that brings the power of Grok directly into your terminal.",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "grok": "dist/index.js"
  },
  "scripts": {
    "build": "bun run script/build.ts",
    "dev": "bun --conditions=browser run src/index.ts",
    "start": "bun run dist/index.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "cli",
    "agent",
    "text-editor",
    "grok",
    "ai",
    "tui",
    "terminal"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@opentui/core": "^0.1.72",
    "@opentui/solid": "^0.1.72",
    "axios": "^1.7.0",
    "cfonts": "^3.3.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.0",
    "fs-extra": "^11.2.0",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "openai": "^5.10.1",
    "ripgrep-node": "^1.0.0",
    "solid-js": "^1.9.0",
    "tiktoken": "^1.0.21",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.0",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.8.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "preferGlobal": true
}
